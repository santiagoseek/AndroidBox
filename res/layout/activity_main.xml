<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.androidbox.MainActivity"
    tools:ignore="MergeRootFrame" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <Button
            android:id="@+id/accessInternet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AccessInternet" />

        <Button
            android:id="@+id/getSystem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AccessSystem" />
        
        <Button
        android:id="@+id/multiProcess"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MultiProcess" />

    </LinearLayout>
</LinearLayout>

